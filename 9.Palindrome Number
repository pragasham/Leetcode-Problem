class Solution {
    public boolean isPalindrome(int x) {
        if(x <0)
        {
            return false;
        }
        int result=0;
        int y=x;
        while(x!=0)
        {
       int rem=x%10;
       result=result*10+rem;
       x/=10;
        }
        if(result==y)
        {
            return true;
        }
        return false;
    }
}
